<?xml version="1.0" encoding="UTF-8"?>
<ContentView
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="DiveSpots.Views.WaterOverviewContentView">

    <ContentView.Content>
        <StackLayout
            Orientation="Horizontal" VerticalOptions="CenterAndExpand">

            <Label FontSize="Subtitle" Text="{Binding WaterName}" />
        </StackLayout>
    </ContentView.Content>
</ContentView>
